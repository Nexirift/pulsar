{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "../shared/tsconfig.node.json",
	"compilerOptions": {
		"noImplicitAny": false,
		"noEmit": true,
		"typeRoots": [
			"./node_modules/@types",
			"./node_modules"
		]
	},
	"include": [
		"*.js",
		"*.ts"
	],
	"exclude": [
		"node_modules",
		"js-built"
	]
}
