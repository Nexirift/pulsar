{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "../shared/tsconfig.node.json",
	"compilerOptions": {
		"noImplicitAny": false,
		"noEmit": true,
		"typeRoots": [
			"./@types",
			"./node_modules/@vue-macros",
			"./node_modules/@types",
			"./node_modules"
		],
		"types": [
			"vite/client",
		]
	},
	"include": [
		"*.js",
		"*.ts"
	],
	"exclude": [
		"node_modules",
		"vue-shims.d.ts"
	]
}
